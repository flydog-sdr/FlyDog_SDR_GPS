#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/board/k3-j721e-bone-pins.h>

#ifdef KiwiSDR
    #define TXDISABLE_SHIFT     (21)
    #define OUTPUT_EN		    (0 << TXDISABLE_SHIFT)
    #define OUTPUT_DISABLE		(1 << TXDISABLE_SHIFT)

    /* k3.h:PIN_INPUT is really a PIN_IO because both rx & tx are enabled */
    #define PIN_IN		        (INPUT_EN | OUTPUT_DISABLE | PULL_DISABLE)
    #define PIN_IN_PULLUP	    (INPUT_EN | OUTPUT_DISABLE | PULL_UP)
    #define PIN_IN_PULLDOWN	    (INPUT_EN | OUTPUT_DISABLE | PULL_DOWN)

    #define PIN_OUT             (INPUT_DISABLE | PULL_DISABLE)
    #define PIN_OUT_PULLUP      (INPUT_DISABLE | PULL_UP)
    #define PIN_OUT_PULLDOWN    (INPUT_DISABLE | PULL_DOWN)

    #define PIN_IO		        (INPUT_EN | PULL_DISABLE)
    #define PIN_IO_PULLUP	    (INPUT_EN | PULL_UP)
    #define PIN_IO_PULLDOWN	    (INPUT_EN | PULL_DOWN)
#endif

