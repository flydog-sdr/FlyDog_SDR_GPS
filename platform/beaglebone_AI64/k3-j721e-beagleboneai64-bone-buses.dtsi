bone_spi_0: &main_spi6 {
    #ifdef KiwiSDR
        pinctrl-names = "default";
        pinctrl-0 = <
            &P9_17_spi_pin /* spi6_cs0 */
            &P9_22_spi_pin /* spi6_sclk */
            &P9_21_spi_pin /* spi6_miso */
            &P9_18_spi_pin /* spi6_d1 */
        >;
        ti,spi-num-cs = <2>;
        #ifdef KiwiSDR
            ti,pindir-d0-in-d1-out;
        #else
            ti,pindir-d0-out-d1-in;
        #endif

        spidev0: spidev@0{
            symlink = "bone/spi/0.0";
            compatible = "spidev";
            reg = <0>;	/* CE0 */
            #address-cells = <1>;
            #size-cells = <0>;
            spi-max-frequency = <125000000>;
        };

        spidev1: spidev@1{
            symlink = "bone/spi/0.1";
            compatible = "spidev";
            reg = <1>;	/* CE1 */
            #address-cells = <1>;
            #size-cells = <0>;
            spi-max-frequency = <125000000>;
        };
    #endif
};

